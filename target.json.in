{
    "arch": "@tgt_arch@",
    "cpu": "@tgt_cpu@",
    "data-layout": "@tgt_layout@",
    "disable-redzone": true,
    "executables": true,
    "llvm-target": "@tgt_llvm_tgt@",
    "morestack": false,
    "os": "@tgt_os@",
    "relocation-model": "static",
    "target-endian": "@tgt_endian@",
    "target-pointer-width": "@tgt_ptr_width@",
    "no-compiler-rt": true,
    "linker": "@CC@",
    "pre-link-args": [
        "-mthumb",
        "-lm",
        "-lgcc",
        "-mcpu=cortex-m3",
        "-Tsrc/hal/lpc17xx/layout.ld"
    ]
}
